{
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "parser": "flow",
    "printWidth": 120,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": true,
    "plugins": ["prettier-plugin-tailwindcss"]
}
